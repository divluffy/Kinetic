{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "linebreak-style": 0,
    "indent": "off",
    "quotes": ["error", "single"],
    "react/react-in-jsx-scope": "off",
    "semi": ["error", "always"]
  }
}
